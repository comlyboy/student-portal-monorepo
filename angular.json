{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"cli": {
		"packageManager": "npm"
	},
	"newProjectRoot": "projects",
	"projects": {
		"portal": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				}
			},
			"root": "projects/portal",
			"sourceRoot": "projects/portal/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "@angular/build:application",
					"options": {
						"browser": "projects/portal/src/main.ts",
						"tsConfig": "projects/portal/tsconfig.app.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							{
								"glob": "**/*",
								"input": "projects/portal/public"
							}
						],
						"styles": [
							"projects/portal/src/styles.scss"
						]
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kB",
									"maximumError": "1MB"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "4kB",
									"maximumError": "8kB"
								}
							],
							"outputHashing": "all"
						},
						"development": {
							"optimization": false,
							"extractLicenses": false,
							"sourceMap": true,
							"fileReplacements": [
								{
									"replace": "projects/portal/src/environments/environment.ts",
									"with": "projects/portal/src/environments/environment.development.ts"
								}
							]
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "@angular/build:dev-server",
					"configurations": {
						"production": {
							"buildTarget": "portal:build:production"
						},
						"development": {
							"buildTarget": "portal:build:development"
						}
					},
					"defaultConfiguration": "development"
				},
				"test": {
					"builder": "@angular/build:unit-test"
				}
			}
		},
		"student-info": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				}
			},
			"root": "projects/student-info",
			"sourceRoot": "projects/student-info/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "@angular/build:application",
					"options": {
						"browser": "projects/student-info/src/main.ts",
						"tsConfig": "projects/student-info/tsconfig.app.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							{
								"glob": "**/*",
								"input": "projects/student-info/public"
							}
						],
						"styles": [
							"projects/student-info/src/styles.scss"
						]
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kB",
									"maximumError": "1MB"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "4kB",
									"maximumError": "8kB"
								}
							],
							"outputHashing": "all"
						},
						"development": {
							"optimization": false,
							"extractLicenses": false,
							"sourceMap": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "@angular/build:dev-server",
					"configurations": {
						"production": {
							"buildTarget": "student-info:build:production"
						},
						"development": {
							"buildTarget": "student-info:build:development"
						}
					},
					"defaultConfiguration": "development"
				},
				"test": {
					"builder": "@angular/build:unit-test"
				}
			}
		},
		"global-search": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				}
			},
			"root": "projects/global-search",
			"sourceRoot": "projects/global-search/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "@angular/build:application",
					"options": {
						"browser": "projects/global-search/src/main.ts",
						"tsConfig": "projects/global-search/tsconfig.app.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							{
								"glob": "**/*",
								"input": "projects/global-search/public"
							}
						],
						"styles": [
							"projects/global-search/src/styles.scss"
						]
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kB",
									"maximumError": "1MB"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "4kB",
									"maximumError": "8kB"
								}
							],
							"outputHashing": "all"
						},
						"development": {
							"optimization": false,
							"extractLicenses": false,
							"sourceMap": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "@angular/build:dev-server",
					"configurations": {
						"production": {
							"buildTarget": "global-search:build:production"
						},
						"development": {
							"buildTarget": "global-search:build:development"
						}
					},
					"defaultConfiguration": "development"
				},
				"test": {
					"builder": "@angular/build:unit-test"
				}
			}
		},
		"shared": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				}
			},
			"root": "projects/shared",
			"sourceRoot": "projects/shared/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "@angular-builders/custom-webpack:browser",
					"options": {
						"customWebpackConfig": {
							"path": "projects/shared/webpack.config.js"
						},
						"main": "projects/shared/src/main.ts",
						"tsConfig": "projects/shared/tsconfig.app.json",
						"outputPath": "dist/shared",
						"index": "projects/shared/src/index.html",
						"assets": [
							"projects/shared/src/favicon.ico",
							"projects/shared/src/assets"
						],
						"styles": [
							"projects/shared/src/styles.scss"
						],
						"scripts": []
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kB",
									"maximumError": "1MB"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "4kB",
									"maximumError": "8kB"
								}
							],
							"outputHashing": "all"
						},
						"development": {
							"optimization": false,
							"extractLicenses": false,
							"sourceMap": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "@angular-builders/custom-webpack:dev-server",
					"options": {
						"browserTarget": "shared:build",
						"customWebpackConfig": {
							"path": "projects/shared/webpack.config.js"
						}
					}
				},
				"test": {
					"builder": "@angular/build:unit-test"
				}
			}
		}
	}
}